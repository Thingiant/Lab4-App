{
	"name": "lab4-app",
	"version": "0.1.0",
	"main": "index.js",
	"scripts": {
		"start": "expo start --dev-client",
		"web": "expo start --web",
		"android": "expo run:android",
		"android:run": "eas build:run --profile development --platform android --latest && expo start --dev-client",
		"android:build": "eas build:run --profile development --platform android",
		"ios": "expo run:ios",
		"ios:build": "eas build --profile development --platform ios",
		"prebuild": "echo y | expo prebuild --clean",
		"tsc": "tsc --noEmit",
		"install-fix": "expo install --fix",
		"doctor": "npx expo-doctor",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" && prettier -c \"src/**/*.{js,jsx,ts,tsx,json}\"",
		"format": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix && prettier \"src/**/*.{js,jsx,ts,tsx,json}\" --write",
		"clean-build": "rm -rf android ios .expo",
		"clean-node": "rm -rf package-lock.json bun.lock yarn.lock node_modules",
		"clean": "bun run clean-build && bun run clean-node"
	},
	"dependencies": {
		"@expo-google-fonts/inter": "^0.4.2",
		"@expo/vector-icons": "^15.0.3",
		"@react-navigation/bottom-tabs": "^7.4.0",
		"@react-navigation/drawer": "^7.5.0",
		"@react-navigation/elements": "^2.6.3",
		"@react-navigation/native": "^7.0.14",
		"@sfourdrinier/react-native-ble-plx": "^3.7.10",
		"@tanstack/react-pacer": "^0.19.1",
		"@tanstack/react-query": "^5.90.16",
		"base-64": "^1.0.0",
		"expo": "~54.0.33",
		"expo-blur": "~15.0.8",
		"expo-constants": "~18.0.13",
		"expo-dev-client": "^6.0.18",
		"expo-font": "~14.0.11",
		"expo-haptics": "~15.0.7",
		"expo-image": "~3.0.10",
		"expo-linear-gradient": "~15.0.7",
		"expo-linking": "~8.0.9",
		"expo-router": "~6.0.23",
		"expo-splash-screen": "~31.0.11",
		"expo-status-bar": "~3.0.8",
		"expo-system-ui": "~6.0.8",
		"expo-updates": "~29.0.16",
		"expo-web-browser": "~15.0.9",
		"jotai": "^2.15.2",
		"moti": "^0.30.0",
		"neverthrow": "^8.2.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.5",
		"react-native-edge-to-edge": "^1.7.0",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-keyboard-controller": "1.18.5",
		"react-native-mmkv": "^4.1.0",
		"react-native-nitro-modules": "^0.32.1",
		"react-native-reanimated": "~4.1.1",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-svg": "15.12.1",
		"react-native-unistyles": "^3.0.21",
		"react-native-web": "^0.21.0",
		"react-native-webview": "13.15.0",
		"react-native-worklets": "0.5.1"
	},
	"devDependencies": {
